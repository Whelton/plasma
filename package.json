{
  "name": "@wework-dev/plasma",
  "version": "0.0.200",
  "description": "",
  "main": "./dist/index.js",
  "scripts": {
    "start": "babel-node server.js",
    "lint": "./node_modules/.bin/eslint src/components/**/*.jsx",
    "pack": "NODE_ENV=production webpack --config webpack.config.prod -p",
    "createRelease": "webpack --config webpack.config.prod && npm publish --access public",
    "build": "webpack --config webpack.config.prod",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -c .storybook -o docs/storybook",
    "preversion": "node ./scripts/preversion.js",
    "prettier": "prettier --write \"src/**/*.{js,jsx}\" \"stories/**/*.{js,jsx}\"",
    "prettier:ci": "prettier -l \"src/**/*.{js,jsx}\" \"stories/**/*.{js,jsx}\"",
    "version": "node ./scripts/version.js && yarn run pack && git add -A dist",
    "postversion": "node ./scripts/postversion.js",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "watch": "webpack --watch --config webpack.config.dev",
    "test": "node_modules/.bin/jest",
    "test:watch": "yarn test --watch --coverage",
    "test:update": "yarn test --updateSnapshot",
    "test:coverage": "yarn test --coverage --coverageReporters=text-lcov",
    "flow:coverage": "flow-coverage-report --config .flow-coverage.config.json --strict-coverage",
    "flow:buildTypeDefs": "node ./scripts/buildFlowTypeDefs.js"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "eslint",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn flow && yarn lint-staged"
    }
  },
  "author": "WeWork",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.4",
    "@babel/plugin-transform-modules-commonjs": "^7.5.0",
    "@babel/plugin-transform-runtime": "^7.5.0",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.4",
    "@babel/runtime": "^7.5.4",
    "@storybook/addon-actions": "^3.1.9",
    "@storybook/addon-links": "^3.1.6",
    "@storybook/react": "^3.4.8",
    "anchorate": "^1.1.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "8.2.6",
    "babel-jest": "^23.4.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-lodash": "^3.2.11",
    "base64-image-loader": "^1.2.0",
    "classnames": "^2.2.5",
    "css-loader": "^0.23.1",
    "enzyme": "3.7.0",
    "enzyme-adapter-react-16": "1.7.0",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "10.0.1",
    "eslint-config-prettier": "4.0.0",
    "eslint-plugin-flowtype": "3.2.1",
    "eslint-plugin-import": "2.15.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "6.10.3",
    "flow-bin": "0.84.0",
    "fs-promise": "^0.5.0",
    "glob": "^7.0.5",
    "husky": "1.3.1",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^3.2.0",
    "jest": "^23.4.2",
    "jest-cli": "^22.0.6",
    "jscodeshift": "^0.6.3",
    "jsdom": "^11.1.0",
    "lint-staged": "8.1.1",
    "lodash": "^4.14.1",
    "marked": "^0.3.6",
    "mkdirp": "^0.5.1",
    "moment": "2.24.0",
    "node-sass": "4.12.0",
    "prettier": "1.16.1",
    "proxy-middleware": "^0.15.0",
    "react": "16.6.3",
    "react-datepicker": "1.8.0",
    "react-docgen-fixed": "^2.9.1",
    "react-dom": "16.6.3",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-styleguidist": "6.4.0",
    "react-test-renderer": "^16.0.0",
    "regenerator-runtime": "^0.10.5",
    "resolve-url-loader": "^1.6.0",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "uuid": "^3.1.0",
    "webpack": "^2.6.0",
    "webpack-bundle-analyzer": "^2.8.2",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "react-select": "2.4.2"
  },
  "peerDependencies": {
    "classnames": "2.2.5",
    "lodash": ">= 4.14 < 5",
    "moment": ">=2.24 < 3",
    "react": ">=16.6 < 17",
    "react-datepicker": "1.8.0",
    "react-dom": ">=16.6 < 17"
  },
  "jest": {
    "testEnvironment": "node",
    "modulePaths": [
      "/shared/vendor/modules"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "shared"
    ],
    "moduleNameMapper": {
      "^config$": "<rootDir>/configs/app-config.js",
      "\\.(css|scss)$": "identity-obj-proxy",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/src/tests/__mocks__/fileMock.js"
    },
    "setupFiles": [
      "<rootDir>/src/tests/setup.js"
    ],
    "testURL": "http://localhost"
  }
}
